#include<iostream>
#include<vector>
#include <sstream>
#include<string>

using namespace std;
int main(){
	string line = "ABhinay is trying to learn c++";
	vector<string> tok;
	stringstream check1(line);
	string inter;
	while(getline(check1,inter,' ')){
		tok.push_back(inter);
	}
	for(int i=0; i<tok.size();i++){
		cout<<tok[i]<<endl;
	}
	return 0;
}
